<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="cd3" class="spring.bean.BlankCD1" p:title="Fantasy" p:artist="Jay Chou">
        <property name="tracks">
            <list>
                <value>Ye de di qi zhang</value>
                <value>Ting mama de hua</value>
                <value>Tui hou</value>
            </list>
        </property>
    </bean>

    <bean id="cdPlayer" class="spring.bean.CDPlayer">
        <constructor-arg ref="cd3"/>    <!--构造器注入-->
    </bean>
</beans>